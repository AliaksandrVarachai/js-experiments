:root {
  /*local variables*/
  --header-color: brown;
  --content-background: yellow;
  --conetne-height: 100px;
}

:global .header {
  color: var(--header-color)
}

:global .content {
  background: var(--content-background);
  height: var(--conetne-height);
}

.container {
  display: flex;
}


:export {
  headerColor: var(--header-color);
  contentBackground: var(--content-background);
  contentHeight: var(--conetne-height);
}
